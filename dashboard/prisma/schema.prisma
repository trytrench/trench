// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["views", "fullTextSearch", "filteredRelationCount", "tracing"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Dataset {
  id        Int      @id @default(autoincrement())
  createdAt DateTime @default(now())

  name        String
  description String    @default("")
  start       DateTime?
  end         DateTime?
  eventCount  Int?

  eventTypes          EventType[]
  eventFeatures       EventFeature[]
  entityFeatures      EntityFeature[]
  entityTypes         EntityType[]
  entities            Entity[]
  events              Event[]
  linkTypes           LinkType[]
  eventToEntityLinks  EventToEntityLink[]
  entityLabelTypes    EntityLabelType[]
  entityLabels        EntityLabel[]
  eventLabelTypes     EventLabelType[]
  eventLabels         EventLabel[]
  eventLabelToEvents  EventLabelToEvent[]
  EntityLabelToEntity EntityLabelToEntity[]
}

model EventType {
  id   String
  name String

  events      Event[]
  eventLabels EventLabel[]

  features EventFeature[]

  dataset   Dataset? @relation(fields: [datasetId], references: [id])
  datasetId Int      @default(0)

  @@id([id, datasetId])
}

model RawEvent {
  id        String   @id @default(dbgenerated("nanoid()"))
  timestamp DateTime
  data      Json     @default("{}")
  type      String
}

model Event {
  id        String   @id @default(dbgenerated("nanoid()"))
  timestamp DateTime
  data      Json     @default("{}")

  eventType EventType @relation(fields: [type, datasetId], references: [id, datasetId])
  type      String

  entityLinks EventToEntityLink[]
  eventLabels EventLabelToEvent[]

  features Json @default("{}")

  dataset   Dataset? @relation(fields: [datasetId], references: [id])
  datasetId Int      @default(0)

  @@index([data(ops: JsonbPathOps)], type: Gin)
  @@index([type, id])
  @@index([timestamp])
  @@index([type, timestamp, id])
  @@index([id, timestamp, type])
}

model EventFeature {
  eventTypeObj EventType @relation(fields: [eventType, datasetId], references: [id, datasetId])
  eventType    String

  name        String
  description String?

  dataset   Dataset? @relation(fields: [datasetId], references: [id])
  datasetId Int      @default(0)

  @@id([eventType, name, datasetId])
}

model EntityFeature {
  entityTypeObj EntityType @relation(fields: [entityType, datasetId], references: [id, datasetId])
  entityType    String

  name        String
  description String?

  dataset   Dataset? @relation(fields: [datasetId], references: [id])
  datasetId Int      @default(0)

  @@id([entityType, name, datasetId])
}

model EntityType {
  id   String
  name String

  entityLabels EntityLabel[]
  entities     Entity[]

  entityFeatures EntityFeature[]

  dataset   Dataset? @relation(fields: [datasetId], references: [id])
  datasetId Int      @default(0)

  @@id([id, datasetId])
}

model Entity {
  id       String @id @default(dbgenerated("nanoid()"))
  name     String
  features Json   @default("{}")

  entityType EntityType @relation(fields: [type, datasetId], references: [id, datasetId])
  type       String

  eventLinks EventToEntityLink[]

  entityLabels EntityLabelToEntity[]

  dataset   Dataset? @relation(fields: [datasetId], references: [id])
  datasetId Int      @default(0)

  @@index([features(ops: JsonbPathOps)], type: Gin)
  @@index([type, id])
}

model LinkType {
  id   String
  name String

  entitySnapshot Json @default("{}")

  eventToEntityLink EventToEntityLink[]
  dataset           Dataset?            @relation(fields: [datasetId], references: [id])
  datasetId         Int                 @default(0)

  @@id([id, datasetId])
}

model EventToEntityLink {
  id        String   @id @default(dbgenerated("nanoid()"))
  createdAt DateTime @default(now())

  linkType LinkType @relation(fields: [type, datasetId], references: [id, datasetId])
  type     String
  data     Json     @default("{}")

  event   Event  @relation(fields: [eventId], references: [id])
  eventId String

  entity   Entity @relation(fields: [entityId], references: [id])
  entityId String

  dataset   Dataset? @relation(fields: [datasetId], references: [id])
  datasetId Int      @default(0)

  @@index([eventId])
  @@index([entityId])
  @@index([entityId, eventId])
  @@index([type, eventId, entityId])
  @@index([type, entityId, eventId])
}

model FileSnapshot {
  id        String   @id @default(dbgenerated("nanoid()"))
  updatedAt DateTime @updatedAt
  createdAt DateTime @default(now())

  code        String
  version     String
  description String?

  file   File?   @relation("fileSnapshot", fields: [fileId], references: [id])
  fileId String?

  currentFile File? @relation("currentFileSnapshot")
}

model File {
  id        String   @id @default(dbgenerated("nanoid()"))
  updatedAt DateTime @updatedAt
  createdAt DateTime @default(now())

  name        String
  description String?

  currentFileSnapshot   FileSnapshot @relation("currentFileSnapshot", fields: [currentFileSnapshotId], references: [id])
  currentFileSnapshotId String       @unique

  fileSnapshots FileSnapshot[] @relation("fileSnapshot")
}

model EntityLabelType {
  id          String   @default(dbgenerated("nanoid()"))
  name        String
  createdAt   DateTime @default(now())
  description String

  entityLabels EntityLabel[]
  dataset      Dataset?      @relation(fields: [datasetId], references: [id])
  datasetId    Int           @default(0)

  @@id([id, datasetId])
}

model EntityLabel {
  id          String   @id @default(dbgenerated("nanoid()"))
  name        String
  createdAt   DateTime @default(now())
  color       String
  description String

  entityTypeObj EntityType @relation(fields: [entityType, datasetId], references: [id, datasetId])
  entityType    String

  labelTypeObj EntityLabelType @relation(fields: [labelType, datasetId], references: [id, datasetId])
  labelType    String

  dataset   Dataset? @relation(fields: [datasetId], references: [id])
  datasetId Int      @default(0)

  entities EntityLabelToEntity[]

  @@unique([name, entityType, labelType, datasetId])
}

model EventLabelType {
  id          String   @default(dbgenerated("nanoid()"))
  name        String
  createdAt   DateTime @default(now())
  description String

  eventLabels EventLabel[]
  dataset     Dataset?     @relation(fields: [datasetId], references: [id])
  datasetId   Int          @default(0)

  @@id([id, datasetId])
}

model EventLabelToEvent {
  id String @default(dbgenerated("nanoid()"))

  eventLabel   EventLabel @relation(fields: [eventLabelId], references: [id])
  eventLabelId String

  event   Event  @relation(fields: [eventId], references: [id])
  eventId String

  dataset   Dataset? @relation(fields: [datasetId], references: [id])
  datasetId Int      @default(0)

  @@id([eventLabelId, eventId, datasetId])
}

model EntityLabelToEntity {
  entityLabel   EntityLabel @relation(fields: [entityLabelId], references: [id])
  entityLabelId String

  entity   Entity @relation(fields: [entityId], references: [id])
  entityId String

  dataset   Dataset? @relation(fields: [datasetId], references: [id])
  datasetId Int      @default(0)

  @@id([entityLabelId, entityId, datasetId])
}

model EventLabel {
  id          String   @id @default(dbgenerated("nanoid()"))
  name        String
  createdAt   DateTime @default(now())
  color       String
  description String

  eventTypeObj EventType @relation(fields: [eventType, datasetId], references: [id, datasetId])
  eventType    String

  labelTypeObj EventLabelType @relation(fields: [labelType, datasetId], references: [id, datasetId])
  labelType    String

  dataset   Dataset @relation(fields: [datasetId], references: [id])
  datasetId Int     @default(0)

  events EventLabelToEvent[]

  @@unique([name, eventType, labelType, datasetId])
}

// Necessary for Next auth
model Account {
  id                String  @id @default(dbgenerated("nanoid()"))
  userId            String
  type              String
  provider          String
  providerAccountId String
  refresh_token     String? // @db.Text
  access_token      String? // @db.Text
  expires_at        Int?
  token_type        String?
  scope             String?
  id_token          String? // @db.Text
  session_state     String?
  user              User    @relation(fields: [userId], references: [id], onDelete: Cascade)

  @@unique([provider, providerAccountId])
}

model Session {
  id           String   @id @default(dbgenerated("nanoid()"))
  sessionToken String   @unique
  userId       String
  expires      DateTime
  user         User     @relation(fields: [userId], references: [id], onDelete: Cascade)
}

model User {
  id            String    @id @default(dbgenerated("nanoid()"))
  name          String?
  email         String?   @unique
  emailVerified DateTime?
  image         String?
  accounts      Account[]
  sessions      Session[]
}

model VerificationToken {
  identifier String
  token      String   @unique
  expires    DateTime

  @@unique([identifier, token])
}
